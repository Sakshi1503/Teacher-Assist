<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teacherassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.teacherassistant.AppBase"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.teacherassistant.profile_activity"
            android:parentActivityName="com.example.teacherassistant.AppBase" />
        <activity
            android:name="com.example.teacherassistant.Student_Registartion"
            android:parentActivityName="com.example.teacherassistant.profile_activity" />
        <activity
            android:name="com.example.teacherassistant.noteCreate"
            android:parentActivityName="com.example.teacherassistant.noteActivity" />
        <activity
            android:name="com.example.teacherassistant.attendanceActivity"
            android:parentActivityName="com.example.teacherassistant.AppBase" />
        <activity
            android:name="com.example.teacherassistant.Result_activity"
            android:parentActivityName="com.example.teacherassistant.cgpa_activity" />
        <activity
            android:name="com.example.teacherassistant.cgpa_activity"
            android:parentActivityName="com.example.teacherassistant.AppBase" />
        <activity
            android:name="com.example.teacherassistant.noteActivity"
            android:parentActivityName="com.example.teacherassistant.AppBase" />
        <activity
            android:name="com.example.teacherassistant.scheduler"
            android:parentActivityName="com.example.teacherassistant.AppBase" />
        <activity
            android:name="com.example.teacherassistant.make_schedule"
            android:parentActivityName="com.example.teacherassistant.scheduler" />
        <activity
            android:name="com.example.teacherassistant.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.example.teacherassistant.AppBase" />
        <activity
            android:name="com.example.teacherassistant.About"
            android:parentActivityName="com.example.teacherassistant.AppBase" />
        <activity android:name="com.example.teacherassistant.Edit_Student"
            android:parentActivityName="com.example.teacherassistant.AppBase"/>
    </application>

</manifest>
